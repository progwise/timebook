{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^fake"]
    },
    "e2e-tests": {
      "dependsOn": ["^fake"]
    },
    "migrate-db": {
      "inputs": ["**/migration.sql"]
    },
    "fake": {
      "inputs": [],
      "outputs": []
    }
  }
}
