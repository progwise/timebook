{
  "$schema": "https://schemas.elgato.com/streamdeck/plugins/manifest.json",
  "PropertyInspectorPath": "propertyInspector/tracking_pi.html",
  "Name": "Timebook",
  "Version": "0.1.0.0",
  "Author": "Progwise",
  "Actions": [
    {
      "Name": "Counter",
      "UUID": "net.progwise.timebook.increment",
      "Icon": "imgs/actions/counter/icon",
      "Tooltip": "Displays a count, which increments by one on press.",
      "Controllers": ["Keypad"],
      "States": [
        {
          "Image": "imgs/actions/counter/key",
          "TitleAlignment": "middle"
        }
      ]
    },
    {
      "Name": "Time tracking",
      "UUID": "net.progwise.timebook.tracking",
      "Icon": "imgs/actions/counter/icon",
      "Tooltip": "Tracks time.",
      "Controllers": ["Keypad"],
      "States": [
        {
          "Image": "imgs/actions/counter/key",
          "TitleAlignment": "middle"
        }
      ]
    }
  ],
  "Category": "Timebook",
  "CategoryIcon": "imgs/plugin/category-icon",
  "CodePath": "bin/plugin.js",
  "Description": "Stream Deck plugin for Timebook",
  "Icon": "imgs/plugin/marketplace",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "6.5"
  },
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion": "10.15"
    },
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "Nodejs": {
    "Version": "20",
    "Debug": "--inspect=127.0.0.1:12345"
  },
  "UUID": "net.progwise.timebook"
}
